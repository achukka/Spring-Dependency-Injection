<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	<bean id="OutputGeneratorHelper" class="com.paypal.java.app.OutputGeneratorHelper">
		<property name="outputGenerator" ref="myRefOutputGenerator" />
	</bean>
	<bean id="myRefOutputGenerator" class="com.paypal.java.impl.OutputGeneratorImpl">
	</bean>
</beans>